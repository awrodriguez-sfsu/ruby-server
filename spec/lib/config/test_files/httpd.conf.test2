#comment should be removed
# comment should be removed

# empty options are invalid
Listen

# inline comments should be removed
Listen # this is an inline comment; line should be ignored

# unrecognized options are invalid
UnrecognizedOption blah1 blah2

# these options are the only mandatory ones in this system
ServerRoot "/absolute/path/to/server/root"
DocumentRoot "/absolute/path/to/document/root"
AccessLog "/absolute/path/to/logs/access_log"
ErrorLog "/absolute/path/to/logs/error_log"
Listen 80

# these options have a default value
AccessFileName #.htaccess
DirectoryIndex #index.html
